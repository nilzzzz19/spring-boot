package com.example.restservice;

public class Product {
     private Long id;
     private String name;
     private Customer customer;
	public Product(Long id, String name, Customer customer) {
		super();
		this.id = id;
		this.name = name;
		this.customer=customer;
	}
	public Long getId() {
		return id;
	}
	public void setId(Long id) {
		this.id = id;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
     
}
